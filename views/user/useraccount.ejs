        function resetPasswordvalidation(event) {
            event.preventDefault();
            console.log("here")

            const newpassword = document.getElementById('newpassword').value;
            const repeatpassword = document.getElementById('repeatpassword').value;

            document.getElementById('newPasswordError').textContent = '';
            document.getElementById('confirmPasswordError').textContent = '';
            

            let isValid = true;


            if (newpassword.length < 8) {
                document.getElementById('newPasswordError').textContent = 'Password must be at least 8 characters';
                isValid = false;
            }

            if (repeatpassword.length < 8) {
                document.getElementById('confirmPasswordError').textContent = 'Password must be at least 8 characters';
                isValid = false;
            }

            if (newpassword !== repeatpassword) {
                document.getElementById('confirmPasswordError').textContent = 'Passwords do not match';
                isValid = false;
            }

            if (!isValid) {
                console.log("your validation failed");
            } else {
                resetPassword();
            }

        }




        // fech request for reset password

        function resetPassword() {
            console.log("viliknund")
            const newpassword = document.getElementById('newpassword').value;
            const currentpassword = document.getElementById('currentpassword').value;
            const repeatpassword = document.getElementById('repeatpassword').value;
            // document.getElementById('resetdiv').textContent = '';

            fetch('/resetpassword', {
                method: 'PUT',
                headers: {
                    "Content-Type": "application/json"
                },
                body: JSON.stringify({
                    newpassword: newpassword,
                    currentpassword: currentpassword,
                    repeatpassword: repeatpassword
                }),
            })
                .then(response => response.json())
                .then(data => {
                    if (data.reseted) {
                        $('#resetdiv').load('/useraccount #resetdiv');

                        // document.getElementById('resetdiv').textContent = 'reset password successfull';

                        // window.relode();
                        // window.location.reload();
                    } else {
                        console.log('Password reset failed');
                    }
                })
                .catch(error => console.error('Error resetting password:', error));
        }

